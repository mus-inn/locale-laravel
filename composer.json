{
    "name": "uselocale/locale-laravel",
    "description": "This is my package locale-laravel",
    "keywords": [
        "uselocale",
        "localization",
        "localisation",
        "laravel",
        "locale-laravel"
    ],
    "homepage": "https://github.com/uselocale/locale-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Creagia",
            "email": "info@creagia.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/contracts": "^7.0|^8.79|^9.0",
        "illuminate/filesystem": "^7.0|^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "nunomaduro/collision": "^4.0|^5.0|^6.0",
        "nunomaduro/larastan": "^1.0.3",
        "orchestra/testbench": "^5.9|^6.23|^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "UseLocale\\LocaleLaravel\\": "src",
            "UseLocale\\LocaleLaravel\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/Support/Utils.php",
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "UseLocale\\LocaleLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "UseLocale\\LocaleLaravel\\LocaleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
